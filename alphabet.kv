<AlphabetMenuScreen>:
    BoxLayout:
        orientation: 'vertical'
        padding: '10dp'
        spacing: '4dp'
        canvas.before:
            Color:
                rgba: 0.8, 0.9, 1, 1
            Rectangle:
                pos: self.pos
                size: self.size
        Label:
            text: 'Choose a Letter:'
            size_hint_y: None
            height: '48dp'
            font_size: '24sp'
            color: 0, 0, 0, 1
        GridLayout:
            cols: 5
            spacing: '10dp'
            padding: '10dp'
            Button:
                text: 'A'
                size_hint_y: None
