        padding: '10dp'
        spacing: '10dp'
        canvas.before:
            Color:
                rgba: 0.8, 0.9, 1, 1 
            Rectangle:
                pos: self.pos
                size: self.size
        Video:
            id: video_v
            source: 'vid_sample_v.mp4' 
            state: 'stop'
            options: {'eos': 'loop'}
            allow_stretch: True
        BoxLayout:
            size_hint_y: None
            height: '48dp'
            Button:
                text: 'Play'
                size_hint: None, None
                size: self.parent.width*0.25, '48dp'
                background_color: 0, 1, 0, 1  
                on_press: video_v.state = 'play'
            Button:
                text: 'Pause'
                size_hint: None, None
                size: self.parent.width*0.25, '48dp'
                background_color: 1, 0, 0, 1 
                on_press: video_v.state = 'pause'
            Button:
                text: 'Stop'
                size_hint: None, None
                size: self.parent.width*0.25, '48dp'
                background_color: 1, 0.5, 0, 1
                on_press:
                    video_v.state = 'stop'
