<VideoAScreen>:
    BoxLayout:
        orientation: 'vertical'
        padding: '10dp'
        spacing: '10dp'
        canvas.before:
            Color:
                rgba: 0.8, 0.9, 1, 1  
            Rectangle:
                pos: self.pos
                size: self.size
        Video:
            id: video_a
            source: 'vid_sample.gif'
            state: 'stop'
            options: {'eos': 'loop'}
            allow_stretch: True
        BoxLayout:
            size_hint_y: None
            height: '48dp'
            Button:
                text: 'Play'
                size_hint: None, None
                size: self.parent.width*0.25, '48dp'
                background_color: 0, 1, 0, 1 
                on_press: video_a.state = 'play'
            Button:
                text: 'Pause'
                size_hint: None, None
                size: self.parent.width*0.25, '48dp'
                background_color: 1, 0, 0, 1  
                on_press: video_a.state = 'pause'
            Button:
                text: 'Stop'
                size_hint: None, None
                size: self.parent.width*0.25, '48dp'
                background_color: 1, 0.5, 0, 1  
                on_press:
                    video_a.state = 'stop'
                    video_a.position = 0 
            Slider:
                min: 0
                max: 1
                value: 1
                step: 0.01
                size_hint_x: 0.8
                pos_hint: {'center_x': 0.5}
                on_value: video_a.volume = self.value
        Button:
            text: 'Back'
            size_hint_y: None
            height: '48dp'
            background_color: 1, 0.5, 0, 1  
            on_press:
                video_a.state = 'stop' 
                root.manager.current = 'alphabet_menu'


